{
  "name": "levis",
  "version": "2.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "import": "cdk8s import",
    "synth": "cdk8s synth",
    "compile": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "build": "yarn compile && npm run test && npm run synth",
    "upgrade": "yarn add cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "yarn add cdk8s@next cdk8s-cli@next",
    "clean": "rm -rf bin manifests dist",
    "build-all": "yarn run clean && yarn run build"
  },
  "dependencies": {
    "cdk8s": "^2.3.20",
    "cdk8s-plus-22": "^2.0.0-rc.12",
    "constructs": "^10.1.32",
    "log4js": "^6.5.2",
    "minimist": "^1.2.6",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.21",
    "cdk8s-cli": "^2.0.14",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.7.3"
  }
}
