{
  "name": "naijabx-levis",
  "version": "1.0.15-alpha",
  "description": "Kubernetes Manifest Generator",
  "keywords": [
    "kubernetes",
    "k8s",
    "manifest",
    "generator"
  ],
  "homepage": "https://github.com/kubeopsskills/levis",
  "main": "main.js",
  "types": "main.ts",
  "license": "MIT",
  "private": false,
  "bin": {
    "naijabx-levis": "./dist/bundle.js"
  },
  "scripts": {
    "import": "cdk8s import",
    "build": "tsc --outDir bin",
    "watch": "tsc -w --outDir bin/dev",
    "clean": "rm -rf bin manifests dist",
    "test": "jest",
    "build-all": "yarn run clean && yarn run build",
    "lint": "eslint . --ext .ts",
    "start": "node bin/main.js",
    "build-rollup": "rimraf dist && tsc --outDir dist && rollup -c rollup.config.js"
  },
  "dependencies": {
    "cdk8s": "^0.30.0",
    "cdk8s-plus": "^0.30.0",
    "constructs": "^3.0.14",
    "log4js": "^6.3.0",
    "minimist": "^1.2.5",
    "yaml": "^2.0.0-6"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^1.10.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.14",
    "@types/minimist": "^1.2.0",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "cdk8s-cli": "^0.30.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.2.0",
    "fs-extra": "^10.0.0",
    "jest": "^26.5.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
